<script id="hbProfileList" type="text/x-handlebars-template">
    {{#each this}}
    <div class="col-sm-4 col-xs-6 vc_{{id}}">
        <label id="al_{{id}}" for="av_{{id}}" class="col-xs-12 configurator-tile " rel-value="profil">
            <input type="radio" value="{{id}}" id="av_{{id}}" name="profil" onclick="updateHelper2(this, '{{id}}', event);" rel-no="0" rel-orig="profil" data-ispreview="true">
            <div class="configurator-image-container">
                <img id="avi_{{id}}" class="img-responsive" alt="{{name}}" src="/Content/img/{{img}}" rel-src="{{img}}" style="max-width:100%;">
            </div>
            <span id="avt_{{id}}">{{name}}</span>
            <div class="small-configurator-tile-comparison">
                <ul class="comparison">
                    {{#each details}}
                    <li>{{this}}</li>
                    {{/each}}
                </ul>
             
            </div>
            <div class="configurator-block-price-spacer">
            </div>
            {{#if isSurcharged}}
            <span id="pl_{{id}}" class="configurator-block-price comparison " style="">{{surcharge}}% surcharge</span>
            <input id="hs_{{id}}" type="hidden" value="{{surcharge}}">
            {{/if}}
        </label>
    </div>
    {{/each}}

</script>