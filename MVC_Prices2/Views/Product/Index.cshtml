                    $('#spanSopraluce').text(sopraluceFark);
                    if (renk !== "0") {
                        if (!wobject.colors[renk].isSurcharged) {
                            if (latoD === "0")
                                factorPrice = Math.ceil(factorPrice * 1.15);
                            else
                                factorPrice = Math.ceil(factorPrice * 1.08);
                        } else {
                            if (latoD === "0") {
                                factorPrice = Math.ceil(factorPrice * 1.15);
                                factorPrice = Math.ceil(factorPrice * 1.05);
                            } else {
                                factorPrice = Math.ceil(factorPrice * 1.08);
                                factorPrice = Math.ceil(factorPrice * 1.05);
                            }
                        }
                    }
                    break;
                }
            }
            factorPrice = Number(fiyat);
        }

        factorPrice = Math.ceil(factorPrice + Math.ceil(m2 * camfiyat) + kolfark + sogBasDiff + cutDiff + yProfilFark+ kornisFark + markaFark);
        setPrice(factorPrice);
    }

