    function getSelectedOne() {
        var tow = document.configuratorForm.a_155.value;
        var sub = document.configuratorForm.a_160.value;
        var op = document.configuratorForm.a_161.value;

        return wobject.products[tow].subTypes[sub].opTypes[op];
    }

    function validateNumericInput(a = null) {
        var isOkay = false;
        if (a != null) {
            a.value = a.value.replace(/\D/g, '');
            var attr = a.getAttribute('name');
            var selected = getSelectedOne();
            let totalValue = +a.value;
            if (attr === "a_width") {// total genişlik ise

                if (a.value >= selected.minWidth && a.value <= selected.maxWidth && a.value % 1 === 0) {
                    a.classList.remove('error');
                    a.classList.add('valid');
                    let wings = $('input[name^="layoutDown"]');
                    let maxWingW = 0;
                    for (let i = 0; i < wings.length; i++) {
                        wings[i].wingType = selected.down[i];
                        if (maxWingW < wings.length * wobject.settings[i].MinW) {
                            maxWingW = wings.length * wobject.settings[i].MinW;
                        }
                    }
                    for (let i = 0; i < wings.length; i++) {
                        let settings = wobject.settings[selected.down[i]];
                        if (a.value >= maxWingW) {//eşit dağıtmaya yetiyor mu
                            if (a.value % wings.length === 0) {
                                wings[i].value = a.value / wings.length;
                            } else {
                                let add = i === 0 ? a.value % wings.length : 0;
                                wings[i].value = (a.value - a.value % wings.length) / wings.length + add;
                            }
                        } else {// eşit değilse kanata mini atıp kalanını dağıt
                            wings[i].value = i === wings.length - 1 ? totalValue : wobject.settings[wings[i].wingType].MinW;
                            totalValue -= wobject.settings[wings[i].wingType].MinW;
                        }

                    }
                    isOkay = true;
                } else {
                    a.classList.add('error');
                    a.classList.remove('valid');
                }
            } else if (attr === "a_height") {//total yükseklik ise
                if (a.value >= selected.minHeight && a.value <= selected.maxHeight && a.value % 1 === 0) {
                    a.classList.remove('error');
                    a.classList.add('valid');
                    isOkay = true;
                } else {
                    a.classList.add('error');
                    a.classList.remove('valid');
                }

            } else if (attr.includes('layout')) {//ek ölçü ise
                if (attr.includes('Right')) {//yükseklik ek ölçüsü ise

                    let opposite = Array('1', '0');
                    let wingAId = attr.replace('layoutRightInp', '');
                    let wingBId = 'layoutRightInp' + opposite[wingAId];
                    let b = document.getElementById(wingBId);

                    let totalHeight = document.configuratorForm.a_height.value;
                    let aSettingIndex = selected.right[wingAId];
                    let bSettingIndex = selected.right[b.getAttribute('rel-id')];
                    let settingsA = wobject.settings[aSettingIndex];
                    let settingsB = wobject.settings[bSettingIndex];
                    if (a.value >= settingsA.MinH && totalHeight - a.value >= settingsB.MinH) {//fix kanat fix min ölçüden büyük & total - diğer parça min ölçüden büyükse
                        b.value = totalHeight - a.value;
                    } else {
                        if (a.value < settingsA.MinH) {
                            a.value = settingsA.MinH;
                            b.value = totalHeight - settingsA.MinH;
                            $('#division-min-warning').show();
                            setTimeout(function () {
                                $('#division-min-warning').fadeOut();
                            }, 8000);
                        } else {
                            a.value = totalHeight - settingsB.MinH;
                            b.value = settingsB.MinH;
                            $('#division-max-warning').show();
                            setTimeout(function () {
                                $('#division-max-warning').fadeOut();
                            }, 8000);
                        }
                    }
                } else if (attr.includes('Down')) {//genişlik ek ölçü ise
                    let totalWidth = document.configuratorForm.a_width.value;
                    let id = a.getAttribute('rel-id');
                    let settingsA = wobject.settings[selected.down[id]];
                    let wings = $('input[name^="layoutDown"]');
                    let sumOfLeft = 0;
                    let sumOfMins = 0;
                    let maxWingW = 0;
                    for (let i = 0; i < wings.length; i++) {
                        wings[i].wingSettings = wobject.settings[selected.down[i]];
                        if (maxWingW < wings.length * wobject.settings[i].MinW) {
                            maxWingW = wings.length * wobject.settings[i].MinW;
                        }
                        sumOfLeft += i <= id ? +wings[i].value : 0;
                        sumOfMins += i > id ? +wings[i].wingSettings.MinW : 0;
                    }
                    if ((a.value >= wings[id].wingSettings.MinW && a.value <= wings[id].wingSettings.MaxW) && totalWidth - sumOfLeft >= sumOfMins) {//girdi uygunsa ve kalan yetiyorsa
                        for (let i = ++id; i < wings.length; i++) {//sonraki inputları dön
                            wings[i].value = i === wings.length - 1 ? totalWidth - sumOfLeft : wings[i].wingSettings.MinW;//eğer son kanatsa kalanı değilse minini alır
                            if (i === wings.length - 1 && wings[i].value > wings[i].wingSettings.MaxW) {
                                let maxDiff = +wings[i].value - wings[i].wingSettings.MaxW;
                                wings[i].value -= maxDiff;
                                id = a.getAttribute('rel-id');
                                for (let j = 0; j < wings.length; j++) {
                                    if (j === +id || j === i) continue;
                                    wings[j].value = +wings[j].value + maxDiff;
                                }
                            }
                            sumOfLeft += wings[i].wingSettings.MinW;
                        }
                    } else {
                        if (a.value < wings[id].wingSettings.MinW) {
                            a.value = wings[id].wingSettings.MinW;
                            sumOfLeft = totalWidth - a.value;
                            for (let i = 0; i < wings.length; i++) {
                                if (i === +id) continue;
                                wings[i].value = i === wings.length - 1 ? sumOfLeft : wings[i].wingSettings.MinW;

                                if (i === wings.length - 1 && wings[i].value > wings[i].wingSettings.MaxW) {
                                    let maxDiff = +wings[i].value - wings[i].wingSettings.MaxW;
                                    wings[i].value -= maxDiff;
                                    for (let j = 0; j < wings.length; j++) {
                                        if (j === +id || j === i) continue;
                                        wings[j].value = +wings[j].value + maxDiff;
                                    }
                                }
                                sumOfLeft -= wings[i].value;

                            }
                        } else {

                            sumOfMins = 0;
                            for (let i = 0; i < wings.length; i++) {
                                if (i === +id) continue;
                                sumOfMins += wings[i].wingSettings.MinW;
                            }
                            if (totalWidth - sumOfMins - a.value < 0) {
                                for (let i = 0; i < wings.length; i++) {
                                    if (i === +id) continue;
                                    wings[i].value = wings[i].wingSettings.MinW;
                                }
                                a.value = totalWidth - sumOfMins;

                            } else {
                                id = a.getAttribute('rel-id');
                                let valueA = totalWidth - sumOfMins;
                                if (valueA > settingsA.MaxW) {
                                    sumOfMins += valueA - settingsA.MaxW;
                                    valueA = settingsA.MaxW;
                                }
                                a.value = valueA;
                                for (let i = 0; i < wings.length; i++) {
                                    if (i === +id) continue;
                                    wings[i].value = i === wings.length - 1 ? sumOfMins : wings[i].wingSettings.MinW;
                                    sumOfMins -= wings[i].value;
                                }
                            }

                            $('#division-max-warning').show();
                            setTimeout(function () {
                                $('#division-max-warning').fadeOut();
                            }, 8000);
                        }
                    }

                }
            }
            checkNextPage();
            return isOkay;

        }
        return false;
    }

    let checkNextPage = () => {
        var w = document.configuratorForm.a_width;
        var h = document.configuratorForm.a_height;
        var ok = true;
        var layout = $("input[name^='layout']");
        var layoutSum = 0;
        for (let i = 0; i < layout.length; i++) {
            if (layout[i].value == "") {
                ok = false;
            } else {
                layoutSum += +layout[i].value;
            }
        }
        if (w.classList.contains('valid') && h.classList.contains('valid') && ok) {
            $('#btnNextPage').prop('disabled', false);
            $('#configurator-step-container').css('display', 'flex');

        }
        else {
            layout.prop('disabled', true);
            $('#btnNextPage').prop('disabled', true);
            $('#configurator-step-container').css('display', 'none');
            ok = false;
        }

        if (w.classList.contains('valid') && h.classList.contains('valid'))
            layout.prop('disabled', false);
        else if (!(w.classList.contains('valid') && h.classList.contains('valid')) && layoutSum !== +w.value)
            layout.val('');

        layout = $("input[name^='layoutDown']");
        if (layout.length > 0)
            layout[layout.length - 1].disabled = true;
        return ok;
    }
</script>


