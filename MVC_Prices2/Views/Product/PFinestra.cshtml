<script type="text/javascript">
    var tempData;
    $(document).ready(function () {
        $('#configurator-step-1,#configurator-step-2').on('click',
            function () {
                var page = this.getAttribute('rel-page');
                nextPage(page);
            });
        CreateProfileList(wobject.profiles);
        CreateColorList(wobject.colors);
        CreateTOWList(wobject.products);
        CreateHandleList(wobject.handles);
        CreateULFLList(wobject.products[0].subTypes);
        CreateULFLList2(wobject.products[0].subTypes[0].fanlight);
        CreateOTList(wobject.products[0].subTypes[0].opTypes);
        CreateMSizes(wobject.products[0].subTypes[0].opTypes[0]);
        CreateULSize(wobject.products[0].subTypes[0].fanlight[0]);

        CreateLayout(wobject.products[0].subTypes[0]);
        document.getElementById('configuratorForm').addEventListener('change',
            function () {
                Hesap();
            });

        let formData = "@Html.Raw(Model.FormData)";
        let formElements = formData.split('&');
        let formElements2= [];
        for (let i = 0; i < formElements.length;i++)
        {
            let object = {};
            let nameVal = formElements[i].split('=');
            object.name=nameVal[0];
            object.value=nameVal[1];
            formElements2[i] = object;

        }
        tempData = formElements2;

        for (let i = 0; i < tempData.length; i++) {
            let objName = tempData[i].name;
            let objValue = tempData[i].value;

            let nElements = $('[name="' + objName + '"]');

            for (let j = 0; j < nElements.length; j++) {
                let nType = nElements[j].getAttribute('type');
                let nValue = nElements[j].getAttribute('value');
                let nId = nElements[j].getAttribute('id');

                if (nType==="radio" || nType==="checkbox") {
                    if (nValue==objValue) {
                        nElements[j].checked = true;
                        if (nElements[j].onclick != null)
                            nElements[j].onclick.call(nElements[j], nId, event);
                        if (nElements[j].onchange != null)
                            nElements[j].onchange.call(nElements[j]);
                    }
                } else if (nType === "text" || nType === "tel" || nType === "hidden" || nType==="select") {
                    nElements[j].value = objValue;
                    if (nElements[j].onkeyup != null)
                        nElements[j].onkeyup.call(nElements[j]);
                    if (nElements[j].onchange != null)
                        nElements[j].onchange.call(nElements[j]);
                }
            }
            Hesap();

        }
    });

    window.onscroll = function () {
        whenScroll(window.pageYOffset);
    };

    function whenScroll(x) {
        if (x < 86) $('#configurator-affix-container').css('margin-top', '-' + x + 'px');
        else $('#configurator-affix-container').css('margin-top', '-86px');
    }


    function updateHelper2(a, b, c) {
        var attr = a.getAttribute('name');
        var oldLabels = $("[rel-value*='" + attr + "']");
        var activeLabel = $('#al_' + b);
        oldLabels.removeClass('active');
        activeLabel.addClass('active');
        switch (attr) {
        case "profil":
            profileProcess();
            break;
        case "a_187":
            directions();
            break;
        case "a_renk":
            applyColor();
            break;
        case "a_185":
            applyColor();
            break;
        case "a_155":
            applyMainType(a);
            break;
        case "a_160":
            applyULType(a);
            break;
        case "a_161":
                applyOpType(a);
                break;

        case "a_flType":
            applyULSize(a);
            break;
        case "a_190":
            showAttrPanel(a);
            break;
        case "a_193":
            showAttrPanel(a);
            break;
        case "a_297":
            showAttrPanel(a);
            break;
        case "a_495":
            showAttrPanel(a);
            break;
        case "armoptions":
                break;
        case "a_188":
            showAttrPanel(a);
                break;
        case "a_fLYesNo":
            showAttrPanel(a);
            break;
        default:
        }
    }

    function profileProcess() {
        var profil = document.configuratorForm.profil.value;
