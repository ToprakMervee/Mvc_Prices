    //handle bars creators
    function Hesap() {
        var selectedOne = getSelectedOne();
        var renk = document.configuratorForm.a_renk.value;
        var fiyat = 0;
        var sogBasDiff = 0;
        var factorPrice = 0;
        var en = document.configuratorForm.a_width.value;
        var boy = document.configuratorForm.a_height.value;
        var profil = document.configuratorForm.profil.value;
        var pricedProfile = document.configuratorForm.pricedProfile.value;
        if (profil !== pricedProfile) {
            GetDatas(selectedOne.id);
            fiyatlar = [];
        }
        var kornisVarmi = document.configuratorForm.a_188.value == "1";
        var kornisFark = 0;
        if (kornisVarmi) {//kornis farkı angolari coprifili
            let cb = getCheckedBoxes('angTaraf');
            let kornisSecimi = document.configuratorForm.a_angcop.value;
            let pFiyatInd = renk === "0" ? "0" : renk > "2" ? "2" : "1";
            let pFiyat = wobject.cornice[kornisSecimi].price[pFiyatInd];

            $('#pl_prCop40').text(wobject.cornice[0].price[pFiyatInd] + ' € (per mt)');
            $('#pl_prAng40').text(wobject.cornice[1].price[pFiyatInd] + ' € (per mt)');
            $('#pl_prCop60').text(wobject.cornice[2].price[pFiyatInd] + ' € (per mt)');
            $('#pl_prAng80').text(wobject.cornice[3].price[pFiyatInd] + ' € (per mt)');
            for (let ij = 0; ij < cb.length; ij++) {
                if (cb[ij] === "Sinistra" || cb[ij] === "Destra") {
                    kornisFark = kornisFark + (en * pFiyat / 1000);

                } else {
                    kornisFark = kornisFark + (boy * pFiyat / 1000);
                }
            }
        }
        if (en == "" || boy == "") {
            return;
        }
        crichetto(selectedOne.id, en, boy);
        var kol = document.configuratorForm.a_kol.value;
        var kolfark = wobject.handles[kol].surcharge;
        var camfiyat = 0;
        var satineCam = document.configuratorForm.a_193.value === "1";
        var camtipi = document.configuratorForm.a_186.value;
        var m2 = en * boy / 1000000;
        var yProfilVarmi = document.configuratorForm.a_297.value == "1";
        var satineEk = 0;
        var ucCamEk = 0;
        var esikFark = 0;
        var esik = document.configuratorForm.a_soglia.value == "1";
        esikFark = Math.ceil(en * 8 / 1000);
        $('#pl_sogliay').text('+'+esikFark+' €');
        if (!esik) {
            esikFark = 0;
        }
        var yProfilFark = 0;
        var sopraluce = document.configuratorForm.a_fLYesNo.value == "1";
        var sopraluceFark = 0;
        var sopraluceBoy = document.configuratorForm.a_sopheight.value;
        var marka = document.configuratorForm.a_ferr.value;
        var markaFark = 0;
        if (yProfilVarmi) {
            var yProfilSecimi = document.configuratorForm.a_495.value;
            var cb = getCheckedBoxes('cpSide');
            for (let ij = 0; ij < cb.length; ij++) {
                let pFiyat = wobject.connection[yProfilSecimi].price;
                if (cb[ij] === "Sinistra" || cb[ij] === "Destra") {
                    yProfilFark = yProfilFark + (en * pFiyat / 1000);

                } else {
                    yProfilFark = yProfilFark + (boy * pFiyat / 1000);
                }
            }
        }

        if (satineCam && camtipi === "0") { //2li satineyse
            camfiyat = 7;
        } else if (camtipi === "1" && !satineCam) { //3lü düz camsa
            if (boy < 1701)
                camfiyat = 12;
            else
                camfiyat = 12;


        } else if (camtipi === "1" && satineCam) { //3lü satine ise
            if (boy < 1701)
                camfiyat = 19;
            else
                camfiyat = 19;

        }
        ucCamEk = (m2 * (boy < 1701 ? 12 : 12)).toFixed(0);
        satineEk = (m2 * 7).toFixed(0);
        $('#pd_satinato').text('+ ' + satineEk + ' €');
        $('#pd_satinato').css('display', 'inline');
        $('#pd_tripleGlass').text('+ ' + ucCamEk + ' €');
        $('#pd_tripleGlass').css('display', 'inline');
        //var sogBasDiff = almesik ? en / 1000 * 8 : 0;
        var cutDiff = 0;
        var kesimmi = document.configuratorForm.a_187.value === "1";
        var latoD = document.configuratorForm.a_185.value;
        if (profil === "1" && kesimmi) {
            cutDiff = getCheckedBoxes("pervazTaraf").length * 2;
        } else if (profil !== "1" && kesimmi) {
            var checkedBoxes = getCheckedBoxes("pervazTaraf");
            for (let ij = 0; ij < checkedBoxes.length; ij++) {
                if (checkedBoxes[ij] === "Sinistra" || checkedBoxes[ij] === "Destra") {
                    cutDiff = Math.ceil(cutDiff + (en * 1.8 / 1000));
                } else {
                    cutDiff = Math.ceil(cutDiff + (boy * 1.8 / 1000));
                }
            }
        }
        if (sopraluce) {
            for (var i = 0; i < fiyatlar2["length"]; i++) {
                if (en > fiyatlar2[i].Width - 100 && en <= fiyatlar2[i].Width && sopraluceBoy > fiyatlar2[i].Height - 100 && sopraluceBoy <= fiyatlar2[i].Height && fiyatlar2[i].Color == false) {
                    sopraluceFark = Math.ceil(Number(fiyatlar2[i].Prices));
                    break;
                }
            }
        }

        let differW = 250;
        let differH = 250;
        let guId = [25, 30, 32, 35, 37];
        let rotoId = [24, 29, 31, 36, 38];
        let id = marka == "1" ? guId : marka == "2" ? rotoId : [0];
        for (var i = 0; i < fiyatlar3["length"]; i++) {

            if (i > 0) {
                if (fiyatlar3[i].Width > fiyatlar3[i - 1].Width) {
                    differW = fiyatlar3[i].Width - fiyatlar3[i - 1].Width;
                }
                if (fiyatlar3[i].Height > fiyatlar3[i - 1].Height) {
                    differH = fiyatlar3[i].Height - fiyatlar3[i - 1].Height;
                }
            } else {
                differW = fiyatlar3[0].Width;
                differH = fiyatlar3[0].Height;
            }
            if (en > fiyatlar3[i].Width - differW && en <= fiyatlar3[i].Width && boy > fiyatlar3[i].Height - differH && boy <= fiyatlar3[i].Height) {

                if (id.includes(fiyatlar3[i].ProductId)) {
                    markaFark = Math.ceil(Number(fiyatlar3[i].Prices));
                }
              
                if (rotoId.includes(fiyatlar3[i].ProductId)) {
                    $('#pd_Roto').text('+'+Math.ceil(Number(fiyatlar3[i].Prices))+'€');
                } else {
                    $('#pd_GU').text('+' +Math.ceil(Number(fiyatlar3[i].Prices)) +'€');
                }
            }
        }

        if (selectedOne.id == "1") {
            kolfark = 0;
            markaFark = 0;

        }
        for (var i = 0; i < fiyatlar["length"]; i++) {
            if (renk === "0" || 1 === 1) {
                if (en > fiyatlar[i].Width - 100 && en <= fiyatlar[i].Width && boy > fiyatlar[i].Height - 100 && boy <= fiyatlar[i].Height && fiyatlar[i].Color == false) {
                    fiyat = fiyatlar[i].Prices;
                    factorPrice = Number(fiyat) + sopraluceFark;
                    $('#spanSopraluce').text(sopraluceFark);
                    if (renk !== "0") {
                        if (!wobject.colors[renk].isSurcharged) {
                            if (latoD === "0")
                                factorPrice = Math.ceil(factorPrice / 0.85);
                            else
                                factorPrice = Math.ceil(factorPrice / 0.92);
                        } else {
                            if (latoD === "0") {
                                factorPrice = Math.ceil(factorPrice / 0.85);
                                factorPrice = Math.ceil(factorPrice / 0.95);
                            } else {
                                factorPrice = Math.ceil(factorPrice / 0.92);
                                factorPrice = Math.ceil(factorPrice / 0.95);
                            }
                        }
                    }
                    break;
                }
            }
            factorPrice = Number(fiyat);
        }

        factorPrice = Math.ceil(factorPrice + (m2 * camfiyat) + kolfark + sogBasDiff + cutDiff + yProfilFark+ esikFark + markaFark);
        setPrice(factorPrice);
    }

    function setPrice(factorPrice) {
